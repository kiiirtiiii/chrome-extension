{
    "name": "Blink Ink",
    "version": "1.0.0",
    "description": "Gives the summery of the webpage",
    "manifest_version": 3,
    "author": "Ishika Jain",
    "action": {
        "default_icon": {
            "16": "favicon.png",
            "48": "favicon.png",
            "128": "favicon.png"
        },
        "default_popup": "index.html"
    },
    "icons": {
        "16": "favicon.png",
        "48": "favicon.png",
        "128": "favicon.png"
    },
    "permissions": [
        "tabs",
        "scripting",
        "contextMenus",
        "activeTab",
        "storage"
    ]
}